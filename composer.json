{
  "name": "vesperphp/start",
  "type": "project",
  "description": "A simple PHP framework for rapid prototyping.",
  "keywords": ["framework", "php", "prototype", "vesper"],
  "license": "MIT",
  
   "authors": [
      {
          "name": "Rik Janssen",
          "email": "rik@vesperphp.com"
      }
  ],
  
  "require": {
    "vesperphp/structure": "0.1"
  },
  
   "autoload": {
    "classmap": [
      "app/",
      "vendor/vesperphp/"
    ]
  },

  "config": {
    "optimize-autoloader": true,
    "prefer-stable": false,
    "minimum-stability": "alpha"
  }
  
}
